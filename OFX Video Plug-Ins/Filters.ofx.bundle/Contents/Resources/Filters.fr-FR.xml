<!DOCTYPE OfxImageEffectResource SYSTEM "ofx.dtd">
 -->
<OfxImageEffectResource>
	<OfxPlugin name="com.vegascreativesoftware:Filters.Stereoscopic3DAdjust">

		<OfxResourceSet ofxHost="default">
			<OfxPropLabel>Réglage 3D stéréoscopique VEGAS</OfxPropLabel>

            <OfxImageEffectPluginPropGrouping>VEGAS</OfxImageEffectPluginPropGrouping>
            <OfxPropPluginDescription>Permet de régler les médias en 3D stéréoscopique.</OfxPropPluginDescription>
            <OfxImageEffectContext name="default">
            </OfxImageEffectContext>

			<OfxImageEffectContext name="OfxImageEffectContextFilter">

				<OfxParamTypeDouble name="Horizontal">
					<OfxPropLabel>Décalage horizontal</OfxPropLabel>
				</OfxParamTypeDouble>
				
				<OfxParamTypeGroup name="AdjustmentsGroup">
                    <OfxPropLabel>Corrections</OfxPropLabel>
                </OfxParamTypeGroup>
				
				<OfxParamTypeDouble name="Vertical">
					<OfxPropLabel>Décalage vertical</OfxPropLabel>
				</OfxParamTypeDouble>
				<OfxParamTypeDouble name="Zoom">
					<OfxPropLabel>Zoom</OfxPropLabel>
				</OfxParamTypeDouble>
				<OfxParamTypeDouble name="Keystone">
					<OfxPropLabel>Distorsion</OfxPropLabel>
				</OfxParamTypeDouble>
				<OfxParamTypeDouble name="Rotation">
					<OfxPropLabel>Rotation</OfxPropLabel>
				</OfxParamTypeDouble>
				<OfxParamTypeChoice name="FlipHorizontal">
					<OfxPropLabel>Retourner à l'horizontale</OfxPropLabel>
					<OfxParamPropChoiceOption>Désactiver</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Gauche</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Droite</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Les deux</OfxParamPropChoiceOption>
				</OfxParamTypeChoice>
				<OfxParamTypeChoice name="FlipVertical">
					<OfxPropLabel>Retourner à la verticale</OfxPropLabel>
					<OfxParamPropChoiceOption>Désactiver</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Gauche</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Droite</OfxParamPropChoiceOption>
					<OfxParamPropChoiceOption>Les deux</OfxParamPropChoiceOption>
				</OfxParamTypeChoice>
                <OfxParamTypePushButton name="AutoAlignButton">
                    <OfxPropLabel>Correction automatique</OfxPropLabel>
                </OfxParamTypePushButton>

                <OfxParamTypeBoolean name="AutoCrop">
                    <OfxPropLabel>Recadrage automatique</OfxPropLabel>
                </OfxParamTypeBoolean>

				<OfxParamTypeDouble name="Crop">
					<OfxPropLabel>Recadrer</OfxPropLabel>
				</OfxParamTypeDouble>

				<OfxParamTypeGroup name="FloatingWindowsGroup">
					<OfxPropLabel>Fenêtres flottantes</OfxPropLabel>
				</OfxParamTypeGroup>
				
				<OfxParamTypeDouble name="FloatingWindowLeft">
					<OfxPropLabel>Gauche</OfxPropLabel>
				</OfxParamTypeDouble>
				<OfxParamTypeDouble name="FloatingWindowRight">
					<OfxPropLabel>Droite</OfxPropLabel>
				</OfxParamTypeDouble>

                <OfxMessage name="CouldNotAutoCorrect">Impossible de trouver assez de similitude entre les images pour faire la correction automatique.</OfxMessage>

                <OfxProgress name="AutoCorrectProgress">Correction automatique en cours...</OfxProgress>
                
			</OfxImageEffectContext>

		</OfxResourceSet>

	</OfxPlugin>
</OfxImageEffectResource>
